import openerp
import re
import codecs
from openerp.osv import fields, osv
from openerp.tools.translate import _

import time

#~ Creaci√≥n de campos
#~ Creating Fields 

class presupuesto(osv.osv):
    _name = 'presupuesto'
    _inherit = 'presupuesto'
    _columns = {
        'idcodigo': fields.char('dv', size=15, requerid=True),
        'namecod': fields.char('namecod', size=25),        
        }
    _order = 'idcodigo'
    
presupuesto()


